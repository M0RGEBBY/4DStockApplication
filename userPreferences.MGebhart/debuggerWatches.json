						"formula": "Form:C1466"
					}
				]
			},
			{
				"path": "CalculatePercentageFromPurchase",
				"watches": [
					{
						"type": 66,
						"formula": "$2"
					},
					{
						"type": 66,
						"formula": "$1"
					},
					{
						"type": 66,
						"formula": "Round:C94(((Num:C11($currentPrice)-Num:C11($purchasePrice))/Num:C11($purchasePrice))*100; 2)"
					}
				]
			},
			{
				"path": "CalculatePercentageFromStrike",
				"watches": [
					{
						"type": 66,
						"formula": "$currentPrice"
					},
					{
						"type": 66,
						"formula": "$percentageFromStrike"
					},
					{
						"type": 66,
						"formula": "$ticker"
					},
					{
						"type": 66,
						"formula": "$finnhubApiKey"
					}
				]
			},
			{
				"path": "calculateStopLossTarget",
				"watches": [
					{
						"type": 66,
						"formula": "$lossTarget"
					},
					{
						"type": 66,
						"formula": "$strikePrice"
					},
					{
						"type": 66,
						"formula": "$strikepric"
					},
					{
						"type": 66,
						"formula": "$strikeprice"
					}
				]
			},
			{
				"path": "FetchStock",
				"watches": [
					{
						"type": 66,
						"formula": "$request"
					},
					{
						"type": 66,
						"formula": "$request.statusCode"
					},
					{
						"type": 66,
						"formula": "$url"
					}
				]
			},
			{
				"path": "GetCompanyLogo",
				"watches": [
					{
						"type": 66,
						"formula": "HTTP Get:C1157($url; $imageBlob)"
					},
					{
						"type": 66,
						"formula": "$url"
					},
					{
						"type": 66,
						"formula": "var $httpRequest:=4D:C1709.HTTPRequest.new($url)"
					},
					{
						"type": 66,
						"formula": "$httpRequest",
						"expanded": true
					},
					{
						"type": 66,
						"formula": "$httpRequest.response"
					}
				]
			},
			{
				"path": "LoadTradingViewChart",
				"watches": [
					{
						"type": 66,
						"formula": "$queryFullName"
					},
					{
						"type": 66,
						"formula": "$tempPath"
					},
					{
						"type": 66,
						"formula": "$filePath"
					}
				]
			},
			{
				"path": "PopulateActivePositions",
				"watches": [
					{
						"type": 66,
						"formula": "(Num:C11($obj.percentageFromPurchasePrice)>0)"
					}
				]
			},
			{
				"path": "populateStockTable",
				"watches": [
					{
						"type": 66,
						"formula": "$url"
					},
					{
						"type": 66,
						"formula": "$request.response.body.results",
						"expanded": true
					}
				]
			},
			{
				"path": "PopulateWatchlist",
				"watches": [
					{
						"type": 66,
						"formula": "$obj.ticker"
					}
				]
			}
		]
