[
	{
		"codeDescriptorWatches": [
			{
				"path": "[projectForm]/AddStockForm/nextButton",
				"watches": [
					{
						"type": 66,
						"formula": "$ticker : "
					},
					{
						"type": 66,
						"formula": "$companyName"
					},
					{
						"type": 66,
						"formula": "$ref1"
					},
					{
						"type": 66,
						"formula": "Form:C1466.Exchange:=Form:C1466.selectedStock.Exchange"
					},
					{
						"type": 66,
						"formula": "Form:C1466.Exchange"
					}
				]
			},
			{
				"path": "[projectForm]/EditWatchlistStockForm/{formMethod}",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466",
						"expanded": true
					}
				]
			},
			{
				"path": "[projectForm]/EditWatchlistStockForm/Button",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466.WatchlistStock",
						"expanded": true
					},
					{
						"type": 66,
						"formula": "Form:C1466.WatchlistStock.UUID"
					}
				]
			},
			{
				"path": "[projectForm]/EditWatchlistStockForm/Input4",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466.WatchlistStock.stopLossPrice"
					}
				]
			},
			{
				"path": "[projectForm]/EditWatchlistStockForm/strikePriceInput",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466.strikePrice"
					},
					{
						"type": 66,
						"formula": "Form:C1466.WatchlistStock.strikePrice"
					},
					{
						"type": 66,
						"formula": "amountText"
					},
					{
						"type": 66,
						"formula": "Form:C1466.amountText"
					},
					{
						"type": 66,
						"formula": "Form:C1466.strikePriceText"
					}
				]
			},
			{
				"path": "[projectForm]/HomeForm/{formMethod}",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466.watchlistStocks",
						"expanded": true
					},
					{
						"type": 66,
						"formula": "$watchlistStock.PercentageFromStrike"
					},
					{
						"type": 66,
						"formula": "String:C10($watchlistStock.PercentageFromStrike; \"###0.00\")+\"%\""
					}
				]
			},
			{
				"path": "[projectForm]/HomeForm/refreshButton",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466.LB_WatchlistStocks"
					},
					{
						"type": 66,
						"formula": "LB_WatchlistStocks"
					},
					{
						"type": 66,
						"formula": "obj.Ticker"
					},
					{
						"type": 66,
						"formula": "$stockEntity.Ticker"
					},
					{
						"type": 66,
						"formula": "$watchlist",
						"expanded": true
					}
				]
			},
			{
				"path": "[projectForm]/HomeForm/removeButton",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466.selectedWatchlistStock"
					},
					{
						"type": 66,
						"formula": "$index"
					}
				]
			},
			{
				"path": "[projectForm]/newStockInfoForm/{formMethod}",
				"watches": [
					{
						"type": 66,
						"formula": "Form:C1466._200DaySMA",
						"expanded": true
					},
					{
						"type": 66,
						"formula": "$alphaRequest.response.body[\"Technical Analysis: SMA\"][\"2025-08-12\"][\"SMA\"]"
					},
					{
						"type": 66,
						"formula": "_200DaySMA; "
					},
					{
						"type": 66,
						"formula": "$alphaQuoteUrl"
					},
					{
						"type": 66,
						"formula": "$webArea"
					},
					{
						"type": 66,
						"formula": "Form:C1466.webArea"
					},
					{
						"type": 66,
						"formula": "Form:C1466.CompanyName"
					},
					{
						"type": 66,
						"formula": "LoadTradingViewChart($ticker; $exchange)"
					},
					{
						"type": 66,
						"formula": "$chartURL"
					}
				]
			},
			{
				"path": "[projectForm]/newStockInfoForm/Button",
				"watches": [
					{
						"type": 66,
						"formula": "$strikePrice"
					},
					{
						"type": 66,
						"formula": "$stopLossPrice"
					},
					{
						"type": 66,
						"formula": "$watchlistStock.PercentageFromStrike"
					},
					{
						"type": 66,
						"formula": "$watchlistStock",
						"expanded": true
					},
					{
						"type": 66,
						"formula": "$watchlistStock.BotResistance"
					},
					{
						"type": 66,
						"formula": "$bottomResistancePrice"
					}
				]
			},
			{
				"path": "CalculatePercentageFromStrike",
				"watches": [
					{
						"type": 66,
						"formula": "$currentPrice"
					},
					{
						"type": 66,
						"formula": "$percentageFromStrike"
					},
					{
						"type": 66,
						"formula": "$ticker"
					},
					{
						"type": 66,
						"formula": "$finnhubApiKey"
					}
				]
			},
			{
				"path": "calculateStopLossTarget",
				"watches": [
					{
						"type": 66,
						"formula": "$lossTarget"
					},
					{
						"type": 66,
						"formula": "$strikePrice"
					},
					{
						"type": 66,
						"formula": "$strikepric"
					},
					{
						"type": 66,
						"formula": "$strikeprice"
					}
				]
			},
			{
				"path": "FetchStock",
				"watches": [
					{
						"type": 66,
						"formula": "$request"
					},
					{
						"type": 66,
						"formula": "$request.statusCode"
					},
					{
						"type": 66,
						"formula": "$url"
					}
				]
			},
			{
				"path": "GetCompanyLogo",
				"watches": [
					{
						"type": 66,
						"formula": "HTTP Get:C1157($url; $imageBlob)"
					},
					{
						"type": 66,
						"formula": "$url"
					},
					{
						"type": 66,
						"formula": "var $httpRequest:=4D:C1709.HTTPRequest.new($url)"
					},
					{
						"type": 66,
						"formula": "$httpRequest",
						"expanded": true
					},
					{
						"type": 66,
						"formula": "$httpRequest.response"
					}
				]
			},
			{
				"path": "LoadTradingViewChart",
				"watches": [
					{
						"type": 66,
						"formula": "$queryFullName"
					},
					{
						"type": 66,
						"formula": "$tempPath"
					},
					{
						"type": 66,
						"formula": "$filePath"
					}
				]
			},
			{
				"path": "populateStockTable",
				"watches": [
					{
						"type": 66,
						"formula": "$url"
					},
					{
						"type": 66,
						"formula": "$request.response.body.results",
						"expanded": true
					}
				]
			},
			{
				"path": "PopulateWatchlist",
				"watches": [
					{
						"type": 66,
						"formula": "$obj.ticker"
					}
				]
			}
		]
	}
]